  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "bun typecheck && next build",
    "start": "next start",
    "lint": "oxlint",
    "lint:fix": "oxlint --fix",
    "format": "oxfmt --write",
    "typecheck": "tsgo --noEmit",
    "auth:generate": "better-auth generate --output src/db/schema/auth-schema.ts --yes",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "bun run --env-file=.env.production drizzle-kit migrate",
    "db:migrate:dev": "bun run --env-file=.env.local db:migrate",
    "db:push": "drizzle-kit push",
    "tools:codename": "bun ./tools/codename-generator.ts"
